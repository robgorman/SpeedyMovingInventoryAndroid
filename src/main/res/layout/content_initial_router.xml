<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    android:id="@+id/content_initial_router"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ranchosoftware.speedymovinginventory.InitialRouterActivity"
    tools:showIn="@layout/activity_initial_router"
    android:background="@color/White">

    <TextView
        android:text="@string/welcome_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@+id/content_initial_router"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="@+id/content_initial_router"
        android:layout_marginTop="232dp"
        app:layout_constraintRight_toRightOf="@+id/content_initial_router"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:id="@+id/textView4"/>

    <ProgressBar
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/progressBar3"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="@+id/content_initial_router"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/content_initial_router"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="256dp"
        app:layout_constraintTop_toTopOf="@+id/content_initial_router"/>
</android.support.constraint.ConstraintLayout>
